import themes.sbss

#web_main {
    width: 1pw;
    height: 100%;
}

#blank_top {
    width: 1pw;
    height: 1sbh+3dp;
    position: absolute;
    gravity: top;
    color: $THEME_COLOR_MAIN;
}

#progress_web {
    width: 1pw;
    height: 3dp;
    position: absolute;
    gravity: top;
    y: 1sbh;
    progress-color: $COLOR_WHITE;
    track-color: $THEME_COLOR_MAIN;
    hidden: yes;
}

/catalog {
    margin-top: 1sbh;
}
