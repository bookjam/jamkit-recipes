import themes.sbss

#effect_equalizer {
    width: 200dp;
    height: 120dp;
    position: absolute;
    gravity: top;
    y: 1sbh+150dp;
}

#btn_action {
    width: 120dp;
    height: 44dp;
    position: absolute;
    gravity: bottom;
    y: -(60dp+1eb);
    label-font-family: $SANS_MEDIUM;
    label-font-size: 1.3;
    content-background-color: $COLOR_DARK_WHITE;
}

/catalog {
    margin-top: 1sbh;
}
